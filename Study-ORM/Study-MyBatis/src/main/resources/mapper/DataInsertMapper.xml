<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nullbugs.mybatis.mapper.DataInsertMapper">


    <insert id="insertData" parameterType="java.util.Map">
        insert into single_table (key1,key2,key3,key_part1,key_part2,key_part3,common_field)values
        (#{key1},#{key2},#{key3},#{key_part1},#{key_part2},#{key_part3},#{common_field})
    </insert>
</mapper>